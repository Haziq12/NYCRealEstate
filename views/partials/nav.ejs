  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>

  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <div class="container-fluid">
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <% if(user) { %> 
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/profiles/<%= user.profile._id %>">
              My Profile
            </a>
          </li>
          <% } %>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/houses">
              Search Houses
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/apartments">
              Search Apartments
            </a>
          </li>  
        </ul>
      </div>
    </div>
    <% if (user) { %>
      <a href="/auth/logout">
        <button type="button" class="btn btn-outline-success logout">Logout</button>
      </a>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>



